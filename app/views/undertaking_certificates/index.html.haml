%p#notice= notice
%h1 Undertaking Certificates
%table
  %thead
    %tr
      %th Protocol
      %th Start date
      %th Title
      %th Amount
      %th Subproject
      %th{:colspan => "3"}
  %tbody
    - @undertaking_certificates.each do |undertaking_certificate|
      %tr
        %td= undertaking_certificate.protocol
        %td= undertaking_certificate.start_date
        %td= undertaking_certificate.title
        %td= undertaking_certificate.amount
        %td= undertaking_certificate.subproject_id
        %td= link_to 'Show', undertaking_certificate
        %td= link_to 'Edit', edit_undertaking_certificate_path(undertaking_certificate)
        %td= link_to 'Destroy', undertaking_certificate, method: :delete, data: { confirm: 'Are you sure?' }
%br/
= link_to 'New Undertaking Certificate', new_undertaking_certificate_path

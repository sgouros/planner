<p id="notice"><%= notice %></p>

<h1>Collective Decision Updates</h1>

<table>
  <thead>
    <tr>
      <th>Protocol</th>
      <th>Start date</th>
      <th>Description</th>
      <th>Comments</th>
      <th>Collective decision</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @collective_decision_updates.each do |collective_decision_update| %>
      <tr>
        <td><%= collective_decision_update.protocol %></td>
        <td><%= collective_decision_update.start_date %></td>
        <td><%= collective_decision_update.description %></td>
        <td><%= collective_decision_update.comments %></td>
        <td><%= collective_decision_update.collective_decision_id %></td>
        <td><%= link_to 'Show', collective_decision_update %></td>
        <td><%= link_to 'Edit', edit_collective_decision_update_path(collective_decision_update) %></td>
        <td><%= link_to 'Destroy', collective_decision_update, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Collective Decision Update', new_collective_decision_update_path %>
